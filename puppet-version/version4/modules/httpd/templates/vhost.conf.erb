<VirtualHost <%= @ipaddress %>:808>
	ServerName maxie.io:808
	ProxyRequests Off
	ProxyVia On
	ProxyPreserveHost On
	<Proxy *>
		Require all granted
	</Proxy>
	ProxyPass	/	http://<%= @ipaddress %>:80/

	<Location />
		Require all granted
	</Location>
